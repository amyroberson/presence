User type 
	-firstName : String *
	-lastName : String *
	-company: String *
	-position: String *
	-email: String *
	-passWord: String *
	-isAdmin : Bool *
	-image: UIimage
	-pictureIsPrivate: Bool
	-imageURL: URL?
	-contacts : []
	-incomeRequests : [requests]
	-sentRequests: [request]
	-expiredRequests: [request]




Event Type
	-name: String
	-location: String
	-time: Date
	-attendants: [User]

RequestType
	toUser: User
	fromUser: User
	isActive: Bool
	time: Date

	.accept()--- adds users to each others contacts
	.reject()--- deletes self from each users requests


	